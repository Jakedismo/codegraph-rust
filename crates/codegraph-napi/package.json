{
  "name": "codegraph",
  "version": "1.0.0",
  "description": "Native Node.js addon for CodeGraph - high-performance code versioning and dependency analysis",
  "main": "index.js",
  "types": "index.d.ts",
  "keywords": [
    "codegraph",
    "versioning",
    "dependency-analysis",
    "native-addon",
    "napi-rs",
    "rust"
  ],
  "author": "CodeGraph Team",
  "license": "MIT",
  "napi": {
    "name": "codegraph",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-unknown-linux-musl",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-apple-darwin",
        "x86_64-pc-windows-msvc"
      ]
    }
  },
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "node test.js",
    "example:graph": "tsx example-graph.ts",
    "universal": "napi universal",
    "version": "napi version"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codegraph/codegraph-rust.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "optionalDependencies": {
    "codegraph-win32-x64-msvc": "1.0.0",
    "codegraph-darwin-x64": "1.0.0",
    "codegraph-darwin-arm64": "1.0.0",
    "codegraph-linux-x64-gnu": "1.0.0",
    "codegraph-linux-x64-musl": "1.0.0",
    "codegraph-linux-arm64-gnu": "1.0.0",
    "codegraph-linux-arm64-musl": "1.0.0"
  }
}
