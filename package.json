{
  "name": "codegraph-mcp-server",
  "version": "1.0.0",
  "description": "CodeGraph Model Context Protocol server for multi-agent coordination",
  "type": "module",
  "main": "dist/server/codegraph-mcp-server.js",
  "bin": {
    "codegraph-mcp": "dist/examples/demo-server.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/examples/demo-server.ts",
    "start": "node dist/examples/demo-server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "demo": "tsx src/examples/demo-server.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "multi-agent",
    "coordination",
    "websocket",
    "code-analysis",
    "ai-agents"
  ],
  "author": "CodeGraph Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/codegraph/mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/codegraph/mcp-server/issues"
  },
  "homepage": "https://github.com/codegraph/mcp-server#readme"
}